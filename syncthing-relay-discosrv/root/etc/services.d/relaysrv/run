#! /usr/bin/with-contenv bash

#启动 relaysrv

exec s6-setuidgid stsrv /usr/local/bin/relaysrv  -global-rate="${GLOBAL_RATE}"  -keys="/config/certs" -message-timeout="${MESSAGE_TIMEOUT}" -network-timeout="${NETWORK_TIMEOUT}"  -per-session-rate="${PER_SESSION_RATE}" -ping-interval="${PING_INTERVAL}"  -pools="${POOLS}" -provided-by="${PROVIDED_BY}"  ${RELAY_OTHER_OPTION} 
